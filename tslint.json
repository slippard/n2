{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "ordered-imports": false,
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "member-access": [
            true,
            "no-public"
        ],
        "trailing-comma": true,
        "max-line-length": [
          true,
          130
        ],
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}
